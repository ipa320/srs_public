<?xml version="1.0"?>
<launch>

  <arg name="exp" default="e1"/>
  <arg name="task" default="nav1"/>
  <arg name="cond" default="b"/>

  <include file="$(find srs_user_tests)/ros/launch/nav_test_remote_common.xml" />

	<!-- Run rviz -->
	<node pkg="rviz" type="rviz" name="rviz" args="-f /map -d $(find srs_user_tests)/ros/config/$(arg exp)/rviz/$(arg cond)/rviz.vcg" />
	
</launch>
