<launch>

  <param name="/use_sim_time" value="true" />

  <!-- Configuration for COB, bringup simulation -->
  <include file="$(find cob_default_robot_config)/upload_param.launch" />
  <include file="$(find cob_default_env_config)/upload_param.launch" />
  <include file="$(find cob_2dnav)/ros/launch/2dnav.launch" />
  <include file="$(find cob_bringup_sim)/launch/robot.launch" />
  
  <!-- start COB script server -->
  <include file="$(find cob_script_server)/launch/script_server.launch"/>
  
  <!-- planning environment -->
  <include file="$(find srs_arm_navigation)/launch/planning_env_but.launch"/>
  
  <!-- actionlibserver -->
  <include file="$(find cob_arm_navigation_but)/launch/arm_manip_node.launch"/>
  
  <!-- planning and filtering -->
  <include file="$(find cob_arm_navigation_but)/launch/planning_filtering.launch"/>
  
  <!-- RVIZ with proper configuration -->
  <node pkg="rviz" type="rviz" name='rviz_warehouse_viewer' args="-d $(find cob_arm_navigation_but)/config/arm_manip_but.vcg">
    <param name="wait_for_start" type="bool" value="false" />
  </node>
  
</launch>
